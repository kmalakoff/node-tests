{
  "name": "fs-compat",
  "version": "0.1.0",
  "description": "Compatibility library to allow the latest fs API to work with previous versions of Node.js",
  "main": "index.js",
  "scripts": {
    "test": "mocha-compat test/spec/**/*.test.js --timeout=100000",
    "compat": "nvs 14,12,10,8,6 mocha-compat test/spec/**/*.test.js",
    "lint": "eslint . --ext js,mjs",
    "format": "prettier --write '**/*.{js,mjs}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kmalakoff/fs-generate.git"
  },
  "keywords": [
    "fs",
    "generate",
    "fixtures"
  ],
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kmalakoff/fs-generate/issues"
  },
  "homepage": "https://github.com/kmalakoff/fs-generate#readme",
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.5",
    "@babel/runtime": "^7.9.6",
    "commander": "^2.20.3",
    "cross-spawn-cb": "^0.1.1",
    "expose-gc": "^1.0.0",
    "fs-iterator": "^3.0.1",
    "fs-stats-spys": "^1.0.1",
    "lodash.some": "^4.6.0",
    "minimatch": "^3.0.4",
    "mkpath": "^1.0.0",
    "mock-require": "kmalakoff/mock-require#master",
    "object.assign": "^4.1.0",
    "ncp": "^2.0.0",
    "queue-cb": "^1.1.2",
    "rimraf": "^2.7.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "mocha-compat": "^3.5.5",
    "nvs": "^0.2.0",
    "prettier": "^2.0.5"
  }
}
